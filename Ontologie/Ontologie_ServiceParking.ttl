@prefix schema: <http://example.org/> .
@prefix geo: <http://example.org/GeoCoordinates/> .
@prefix postalAddress: <http://example.org/PostalAddress/> .
@prefix parking: <http://example.org/Parking/> .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.

# The different schema
schema:Place rdf:type rdfs:Class.
schema:Parking rdf:type rdfs:Class;
				rdfs:subClassOf schema:Place.
schema:PostalAddress rdf:type rdfs:Class.
schema:GeoCoordinates rdf:type rdfs:Class.


# The properties of PostalAddress
postalAddress:streetAddress	rdf:type owl:DatatypeProperty;
							rdfs:range rdfs:Literal;
							rdfs:domain schema:PostalAddress .
						
postalAddress:addressLocality	rdf:type owl:DatatypeProperty;
								rdfs:range rdfs:Literal;
								rdfs:domain schema:PostalAddress .
						
postalAddress:postalCode	rdf:type owl:DatatypeProperty;
							rdfs:range rdfs:Literal;
							rdfs:domain schema:PostalAddress .
							
# The properties of GeoCoordinates
geo:longitude	rdf:type owl:DatatypeProperty;
				rdfs:range rdfs:Literal;
				rdfs:domain schema:GeoCoordinates .
				
geo:latitude	rdf:type owl:DatatypeProperty;
				rdfs:range rdfs:Literal;
				rdfs:domain schema:GeoCoordinates .

# The properties of Parking schema
parking:id	rdf:type owl:DatatypeProperty;
			rdfs:range rdfs:Literal;
			rdfs:domain schema:Parking .

parking:name	rdf:type owl:DatatypeProperty;
				rdfs:range rdfs:Literal;
				rdfs:domain schema:Parking .
		
parking:libCategorie	rdf:type owl:DatatypeProperty;
						rdfs:range rdfs:Literal;
						rdfs:domain schema:Parking .

parking:libType	rdf:type owl:DatatypeProperty;
				rdfs:range rdfs:Literal;
				rdfs:domain schema:Parking .

parking:address rdf:type owl:ObjectProperty;
				rdfs:range schema:PostalAddress;
			  	rdfs:domain schema:Place .

parking:townCode	rdf:type owl:DatatypeProperty;
					rdfs:range rdfs:Literal;
					rdfs:domain schema:Parking .
			
parking:phone	rdf:type owl:DatatypeProperty;
				rdfs:range rdfs:Literal;
				rdfs:domain schema:Parking .

parking:webSite	rdf:type owl:DatatypeProperty;
				rdfs:range rdfs:Literal;
				rdfs:domain schema:Parking .

parking:geo rdf:type owl:ObjectProperty;
			rdfs:range schema:GeoCoordinates;
		  	rdfs:domain schema:Place .

parking:presentation	rdf:type owl:DatatypeProperty;
						rdfs:range rdfs:Literal;
						rdfs:domain schema:Parking .

parking:capacityCar	rdf:type owl:DatatypeProperty;
					rdfs:range rdfs:Literal;
					rdfs:domain schema:Parking .

parking:capacityPmr	rdf:type owl:DatatypeProperty;
					rdfs:range rdfs:Literal;
					rdfs:domain schema:Parking .

parking:capacityElectricCar	rdf:type owl:DatatypeProperty;
							rdfs:range rdfs:Literal;
							rdfs:domain schema:Parking .

parking:capacityBike	rdf:type owl:DatatypeProperty;
						rdfs:range rdfs:Literal;
						rdfs:domain schema:Parking .

parking:serviceBike	rdf:type owl:DatatypeProperty;
					rdfs:range rdfs:Literal;
					rdfs:domain schema:Parking .

parking:otherMobilityProximityService	rdf:type owl:DatatypeProperty;
										rdfs:range rdfs:Literal;
										rdfs:domain schema:Parking .

parking:services	rdf:type owl:DatatypeProperty;
					rdfs:range rdfs:Literal;
					rdfs:domain schema:Parking .

parking:paymentAccepted	rdf:type owl:DatatypeProperty;
						rdfs:range rdfs:Literal;	
						rdfs:domain schema:Parking .

parking:accessConditions	rdf:type owl:DatatypeProperty;
							rdfs:range rdfs:Literal;
							rdfs:domain schema:Parking .

parking:exploiting	rdf:type owl:DatatypeProperty;
					rdfs:range rdfs:Literal;
					rdfs:domain schema:Parking .