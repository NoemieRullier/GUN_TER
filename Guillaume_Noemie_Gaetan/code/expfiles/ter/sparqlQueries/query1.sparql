PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX onto: <http://example.org/>
SELECT *
WHERE 
{
	?lieu onto:hasName ?name .
	?lieu onto:hasAddress ?address .
	?lieu onto:hasPostalCode ?postalCode .
	?lieu onto:hasTown ?town .
	?lieu onto:hasWebSite ?webSite .
	?lieu onto:hasMail ?mail .
}
